<div class="center-align">
	<p>Escoja un servicio para comenzar</p>
	<%for (var category in services){%>
		<h6 class="green-text"><b><%=category.toUpperCase()%></b></h6>
		<div class="row">
		<%services[category].forEach((service) => {%>
			<div class="col s4 m2" title="<%=service.description%>">
				<a href="#" onclick="apretaste.send({command : '<%=service.name%>'})">
				<img src="<%=service.image%>" alt="<%=service.name%>" width="100%">
				<%=service.name%>
				</a>
			</div>
		<%});%>
		</div>
		<div class="divider"></div>
	<%};%>
</div>