<div class="row">
	<div class="col s12 input-field">
		<select id="filter" onchange="filter(value)">
			<option value="" selected>Todo (<%= services.length %>)</option>
			<% Object.keys(categories).forEach(function(key){ %>
				<% var item = categories[key] %>
				<option value="<%= item.code %>"><%= item.name %> (<%= item.count %>)</option>
			<% }) %>
		</select>
	</div>
</div>

<div class="row">
	<% services.forEach(function(item){ %>
		<div class="col s4 m2 center-align service <%= item.category %>">
			<div class="card" style="padding:1rem">
				<a href="#" class="green-text apretaste" onclick="apretaste.send({command:'<%= item.name %>'})">
					<img src="{{APP_IMAGE_PATH}}<%= item.name %>.png" alt="<%= item.caption %>" width="100%">
					<%= item.caption %>
				</a>
			</div>
		</div>
	<% }) %>
</div>